{
    "name": "icomefromthenet/faker",
    "description": "Test data generator using Doctrine DBAL and Symfony2 components",
    "keywords": ["database", "testing","generator"],
    "homepage": "https://github.com/icomefromthenet/Faker",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Lewis Dyer",
            "email": "getintouch@icomefromthenet.com",
            "homepage": "www.icomefromthenet.com"
        }
    ],
    "config": {
        "minimum-stability": "dev" 
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.zendframework.com/"
        },
        {
            "type": "vcs",
            "url": "https://github.com/icomefromthenet/PHPStats"
        }
    ],
    "require": {
        "zendframework/zend-progressbar": "2.0.*",
        "zendframework/zend-stdlib"     : "2.0.*",
        "zendframework/zend-text"       : "2.0.*",
        "monolog/monolog"               : "1.2.1",
        "symfony/filesystem"            : "2.1.2",
        "symfony/class-loader"          : "2.1.2",
        "symfony/config"                : "2.1.2",
        "symfony/console"               : "2.1.2",
        "symfony/event-dispatcher"      : "2.1.2",
        "symfony/http-kernel"           : "2.1.2",
        "symfony/http-foundation"       : "2.1.2",
        "symfony/finder"                : "2.1.2",
        "symfony/yaml"                  : "2.1.2",
        "twig/twig"                     : "1.10.0",
        "pimple/pimple"                 : "1.0.0",
        "doctrine/common"               : "2.3.0",
        "doctrine/dbal"                 : "2.3.0",
        "ext-dom": "*",
        "ext-spl": "*",
        "ext-pcre": "*"
    },
    "autoload": {
        "psr-0": {
            "Faker": "src",
            "PHPStats" : "src"
            }
    },
    "bin" : ["bin/faker.php"]
}