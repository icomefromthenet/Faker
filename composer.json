{
    
    "config": {
        "vendor-dir" : "Faker/Vendor",
        "process-timeout" : "800",
        "minimum-stability": "dev" 
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.zendframework.com/"
        },
        {
            "type": "vcs",
            "url": "https://github.com/icomefromthenet/PHPStats"
        }
    ],
    "require": {
        "zendframework/zend-progressbar": "2.0.*",
        "zendframework/zend-stdlib"     : "2.0.*",
        "zendframework/zend-text"       : "2.0.*",
        "monolog/monolog"               : "dev-master",
        "symfony/filesystem"            : "2.1.x-dev",
        "symfony/class-loader"          : "2.1.x-dev",
        "symfony/config"                : "2.1.x-dev",
        "symfony/console"               : "2.1.x-dev",
        "symfony/event-dispatcher"      : "2.1.x-dev",
        "symfony/http-kernel"           : "2.1.x-dev",
        "symfony/http-foundation"       : "2.1.x-dev",
        "symfony/finder"                : "2.1.x-dev",
        "symfony/yaml"                  : "2.1.x-dev",
        "twig/twig"                     : "dev-master",
        "pimple/pimple"                 : "1.0.x-dev",
        "doctrine/common"               : "2.3.x-dev",
        "doctrine/dbal"                 : "2.3.x-dev",
        "PHPStats/PHPStats"             : "dev-master"
    }
}